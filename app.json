{
  "expo": {
    "name": "Perga App",
    "slug": "pergaapp",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#005f8f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pergaapp.app"
    },
    "android": {
      "package": "com.pergaapp.app"
    },
    "androidStatusBar": {
      "backgroundColor": "#005f8f"
    },
    "packagerOpts": {
      "sourceExts": ["js", "json", "jsx"]
    }
  }
}
